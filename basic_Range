/*
sum of N numbers
5 => 15
*/

#include <iostream>
#include<string>
using namespace std;

int main()
{
  int num,sum=0;
  cin >>  num;
  for(int i=0;i<=num;i++)
  {
     sum = sum + i; 
  }
cout<<sum;
    return 0;
}

