/*
Write a program to find sum of all prime numbers between 1 to n.
10 20
11 13 17 19
60
*/

#include <iostream>

using namespace std;

int main()
{
int a,b,count=0,sum=0,i,j;
cin >> a >> b;
for(i=a;i<=b;i++)
{
    for(j=1;j<=i;j++)
    {
        if(i%j==0)
        {
            count++;
        }
    }
    if(count == 2)
    {
            sum = sum + i;
        
    }
    count = 0;
}
cout << sum << " ";
}
