/*
Write a program to print all odd numbers between 1 to 100. - using while loop
*/
#include <iostream>
#include<string>
using namespace std;

int main()
{
  for(int i=1;i<=100;i++)
  {
     if(i%2!=0) //odd
     {
         cout<<i<<" ";
     }
  }

    return 0;
}

