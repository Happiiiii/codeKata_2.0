/*
Write a program to print multiplication table of any number.
 2 5
1*2=2                                                                                                                          
2*2=4                                                                                                                          
3*2=6                                                                                                                          
4*2=8 
5*2=10
*/

#include <iostream>

using namespace std;

int main()
{
    int table,num;
    cin >> table >> num;
    for(int  i=1;i<=num;i++)
    {
      cout << i << '*' << table << '=' << i*table << endl; 
    }
    return 0;
}
