/*
Write a program to find all factors of a number.
6 
1 2 3 6
*/
#include <iostream>
using namespace std;
int main()
{
  int num;
  cin >> num;
  for(int i=1;i<=num;i++)
  {
    if(num%i==0)
    {
        cout << i <<" ";
    }
  }
}
