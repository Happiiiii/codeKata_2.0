/*
Write a program to find sum of all even numbers between 1 to n.

i/p => 10                                                                                                                             
o/p => 2 4 6 8 10                                                                                                                     
       30 
*/
#include <iostream>
#include<string>
using namespace std;

int main()
{
  int num,sum=0,temp=0;
  cin >>  num;
  for(int i=1;i<=num;i++)
  {
    if(i%2==0) //even
    {
      cout<<i<<" ";
      temp = temp + i;
       
    }
  }
  cout<<endl<<temp;

    return 0;
}

