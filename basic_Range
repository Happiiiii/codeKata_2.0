/*
Write a program to calculate sum of digits of a number
123
1+2+3=6
*/
#include<iostream>
using namespace std;
 
int main()
{
    int  num, sum = 0,rem;
    cin >> num;
    while (num > 0)
    {
        rem = num % 10;
        sum = sum + rem;
        num = num / 10;
    }
    cout  << sum;
}
----------------------------------------------------------------------------------------------------------------------------------------
/*
Write a program to calculate product of digits of a number.
123
1*2*3=6
*/
#include<iostream>
using namespace std;
 
int main()
{
   int num,product=1,rem;
   cin >> num;
   while(num>0)
   {
       rem = num % 10;   
       product = product * rem;
       num = num / 10;
   }
   cout << product;
}
---------------------------------------------------------------------------------------------------------------------------------------
/*
Write a program to enter a number and print its reverse.
12345
54321
*/
#include<iostream>
using namespace std;
 
int main()
{
   int num,rev=0,rem;
   cin >> num;
  while(num>0)
  {
      rem=num%10;
      rev=rev*10+rem;
      num=num/10;
  }
  cout<<rev;
}
