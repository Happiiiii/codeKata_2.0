/*
Write a program to check whether a number is Prime number or not.
7 
prime
*/
#include <iostream>
using namespace std;
int main()
{
 int num,count=0;
 cin >> num;
 for(int i=1;i<=num;i++)
 {
     if(num%i==0)
     {
         count++;
     }
 }
 if(count == 2)
 {
     cout << "prime";
 }
 else
 {
     cout << "not a prime";
 }
}
---------------------------------------------------------------------------------------------------------------------------------------
/*
Write a program to print all Prime numbers between 1 to n.
10 20
11 13 17 19
*/
#include <iostream>

using namespace std;

int main()
{
  int a,b,count=0,i,j;
  cin >> a >> b;
  for(i=a;i<=b;i++)
  {
      for(j=1;j<=i;j++)
      {
          if(i%j==0)
          {
              count++;
          }
      }
      if(count == 2)
      {
          cout << i <<" ";
      }
      count = 0;
  }
}

