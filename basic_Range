/*
Write a program to find power of a number using for loop.
3 3
base * exponent = 27
*/
#include <iostream>
using namespace std;
int main()
{
   int base,expo,temp=1,i;
   cin >> base >> expo;
   for(i=1;i<=expo;i++)
   {
       temp = temp * base;
   }
   cout <<" base * expo = " << temp;
}
