/*
Write a program to find sum of first and last digit of a number.
123
1 3
1+3=4
*/
#include <iostream>
using namespace std;

int main()
{
  int num,fst,lst;
  cin >> num;
  fst = num;
  lst = num % 10;
  while(fst>=10)
  {
      fst = fst/10;
  }
  cout << fst << " " << lst << endl;
  cout << fst+lst ;
    return 0;
}
