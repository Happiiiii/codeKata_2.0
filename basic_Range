/*
Write a program to enter a number and print it in words.
7 
seven
*/

#include <iostream>

using namespace std;

int main()
{
  int num;
  cin >> num;
  switch(num)
  {
      case 1: cout << "one";
      break;
      case 2: cout << "two";
      break;
      case 3: cout << "three";
      break;
      case 4: cout << "four";
      break;
      case 5: cout << "five";
      break;
      case 6: cout << "six";
      break;
      case 7: cout << "seven";
      break;
      case 8: cout << "eight";
      break;
      case 9: cout << "nine";
      break;
  }
    return 0;
}
