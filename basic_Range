/*
Write a program to calculate factorial of a number.
5
1*2*3*4*5 = 120 
*/
#include <iostream>
using namespace std;
int main()
{
 int num,fact=1;
 cin >> num;
 for(int i=1;i<=num;i++)
 {
     fact = fact * i;
 }
 cout << fact;
}
