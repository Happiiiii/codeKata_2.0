/*
Write a program to find sum of all odd numbers between 1 to n.
i/p => 10                                                                                                                             
o/p => 1 3 5 7 9                                                                                                                      
       25  
*/

#include <iostream>
#include<string>
using namespace std;

int main()
{
int num,temp=0;
cin>>num;
for(int i=0;i<num;i++)
{
    if(i%2!=0) //odd
    {
        cout << i << " ";
        temp = temp + i;
    }
}
cout << endl << temp;
    return 0;
}

